{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://wit-back.kro.kr/:path*"
    }
  ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ]
  }